    "axe-core": "^4.11.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-sonarjs": "^3.0.5",
    "jsdom": "^27.4.0",
