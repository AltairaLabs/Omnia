    "openid-client": "^6.8.1",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.70.0",
    "react-image-crop": "^11.0.10",
    "react-markdown": "^10.1.0",
    "recharts": "^3.6.0",
    "remark-gfm": "^4.0.1",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.4.0",
    "tar-stream": "^3.1.7",
    "ws": "^8.18.0",
