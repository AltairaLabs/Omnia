{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev --turbopack",
    "build": "next build",
    "start": "node server.js",
    "start:next": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:junit": "vitest run --reporter=junit --outputFile=test-results.xml",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:coverage": "COLLECT_COVERAGE=true playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:coverage-report": "open e2e-coverage/report.html",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:multimodal": "playwright test --config=playwright.multimodal.config.ts",
    "test:e2e:multimodal:coverage": "COLLECT_COVERAGE=true playwright test --config=playwright.multimodal.config.ts",
    "generate:api": "openapi-typescript ../api/openapi/openapi.yaml -o src/lib/api/schema.d.ts",
    "generate:proto": "./scripts/generate-proto.sh",
    "generate:api-key": "node scripts/generate-api-key.mjs",
    "screenshots": "playwright test --project=screenshots",
    "videos": "playwright test --project=video-capture",
    "screenshots:all": "playwright test --project=screenshots && playwright test --project=video-capture && ./scripts/convert-videos.sh"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.2",
    "@hookform/resolvers": "^5.2.2",
    "@kubernetes/client-node": "^1.4.0",
    "@monaco-editor/react": "^4.7.0",
